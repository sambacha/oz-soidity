{
  "manifestVersion": "2.2",
  "contracts": {},
  "dependencies": {},
  "name": "solidity-typescript-template",
  "version": "1.0.0",
  "compiler": {
    "compilerSettings": {
      "optimizer": {
        "enabled": false,
        "runs": "200"
      }
    },
    "typechain": {
      "enabled": true,
      "outDir": "./types/contracts/",
      "target": "truffle"
    },
    "manager": "openzeppelin",
    "solcVersion": "0.6.11",
    "artifactsDir": "build/contracts",
    "contractsDir": "contracts"
  },
  "telemetryOptIn": false
}
